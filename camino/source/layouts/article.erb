<% wrap_layout :layout do %>
<article class="post">
	<header>
		<div class="title">
			<h2><%= link_to current_article.title, current_article %></h2>
		</div>

	</header>
<a href="#" class="image featured"><%= image_tag current_article.data.image %></a>
  <%= current_article.body %>
</article>

<% end %>
